<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Communication/etc/communication.xsd">
    <topic name="smile.onestock.order.export" request="string">
        <handler name="smile.onestock.order.export" type="Smile\Onestock\Model\Queue\OrderExportConsumer" method="process" />
    </topic>
    <topic name="smile.onestock.shipment.import" request="string">
        <handler name="smile.onestock.shipment.import" type="Smile\Onestock\Model\Queue\ShipmentImportConsumer" method="process" />
    </topic>
</config>
