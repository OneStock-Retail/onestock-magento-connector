<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="onestock">
        <job name="onestock_full_stock_import" instance="Smile\Onestock\Model\Import\StockFull" method="start">
            <schedule>0 4 * * *</schedule>
        </job>
        <job name="onestock_diff_stock_import" instance="Smile\Onestock\Model\Import\StockDiff" method="start">
            <schedule>*/15 6-20 * * *</schedule>
        </job>
    </group>
</config>
